<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="sistemapagoimpuestos.Entity.Empresa"   table="Empresa">
      <id name="OID" type="string"> 
          <column name="OIDEmpresa"/>    
          <generator class="uuid2"/>
      </id> 
          
      <property name="nombreEmpresa"    type="string"> 
      <column name="nombreEmpresa"/>
      </property>
      
      <property name="cuitEmpresa"    type="string"> 
      <column name="cuitEmpresa"/>
      </property>
          
      <property name="direccionEmpresa"    type="string"> 
      <column name="direccionEmpresa"/>
      </property>    
          
      <property name="fechaHoraInhabilitacionEmpresa"   type="date">    
      <column name="fechaHoraInhabilitacionEmpresa"/>        
      </property>
      
     <many-to-one name="tipoEmpresa" class="sistemapagoimpuestos.Entity.TipoEmpresa" column="OIDTipoEmpresa" not-null="true"/>
      
  </class>            
</hibernate-mapping>
