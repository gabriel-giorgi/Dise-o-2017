<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="sistemapagoimpuestos.Entity.ParametroCalculoEditable" table="parametro_calculo_editable">
        
      <id name="OID" type="string">
            <column name="OIDParametroCalculoEditable"/>
            <generator class="uuid2"/>
        </id>
       
         <property name="fechaDesde" type="date" not-null="true">
        <column name="fechaDesde"/>
        </property>
        
        <property name="noEditable" type="double" not-null="true">
            <column name="noEditable"/>
        </property>
        
        <property name="siEditable" type="double" not-null="true">
            <column name="siEditable"/>
        </property>
   
    </class>
</hibernate-mapping>
