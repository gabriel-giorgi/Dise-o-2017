<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sistemapagoimpuestos.Entity.TipoUsuario" table="tipo_usuario">
        <id name="OID" type="string">
            <column name="OIDTipoUsuario"/>
            <generator class="uuid2"/>
        </id>
        
        <property name="codigoTipoUsuario" type="int">
            <column name="codigoTipoUsuario"/>
        </property>

        <property name="nombreTipoUsuario" type="string">
            <column name="nombreTipoUsuario"/>
        </property>
        
        <property name="fechaHoraInhabilitacionTipoUsuario" type="date">
            <column name="fechaHoraInhabilitacionTipoUsuario"/>
        </property>
    </class>
</hibernate-mapping>
