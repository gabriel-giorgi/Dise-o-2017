<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sistemapagoimpuestos.Entity.TipoDatoItem" table = "tipo_dato_item">
         <id name="OID" type="string">
            <column name="OIDTipoDatoItem"/>
            <generator class="uuid2"/>
        </id>
        
        <property name="codigoTipoDatoItem" type="int" unique="true" not-null="true">
            <column name="codigoTipoDatoItem"/>
        </property>
        
        <property name="nombreTipoDatoItem" type="string" not-null="true">
            <column name="nombreTipoDatoItem"/>
        </property>
        
    </class>
</hibernate-mapping>
