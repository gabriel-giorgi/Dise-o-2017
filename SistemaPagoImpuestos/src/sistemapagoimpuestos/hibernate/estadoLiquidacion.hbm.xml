<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sistemapagoimpuestos.Entity.EstadoLiquidacion" table="estado_liquidacion">
        <id name="OID" type="string">
            <column name="OIDEstadoLiquidacion"/>
            <generator class="uuid2"/>
        </id>
        
        <property name="nombreEstadoLiquidacion" type="string" not-null="true">
            <column name="nombreEstadoLiquidacion"/>
        </property>
        
    </class>
</hibernate-mapping>
