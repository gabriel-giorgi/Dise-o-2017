<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="sistemapagoimpuestos.Entity.Periodicidad" table="Periodicidad">
        <id name="OID" type="string">
            <column name="OIDPeriodicidad"/>
            <generator class="uuid2"/>
        </id>
        <property name="cantDiasPeriodicidad" type="int" unique="true" not-null="true">
            <column name="cantDiasPeriodicidad"/>
        </property>      
        
        <property name="tipoPeriodicidad" type="string" unique="true" not-null="true">
            <column name="tipoPeriodicidad"/>
        </property>
        
         <property name="fechaInhabilitacionPeriodicidad" type="date">
            <column name="fechaInhabilitacionPeriodicidad"/>
        </property>
    </class>
</hibernate-mapping>
