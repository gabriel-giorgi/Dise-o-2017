<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="sistemapagoimpuestos.Entity.EmpresaTipoImpuesto" table="empresa_tipo_impuesto">
        <id name="OID" type="string">
            <column name="OIDEmpresaTipoImpuesto"/>
            <generator class="uuid2"/>
        </id>
        <property name="urlConexionEmpresaTipoImpuesto" type="string" unique="true" not-null="true">
           <column name="urlConexionEmpresaTipoImpuesto"/>
        </property>    
        <property name="fechaHoraInhabilitacionEmpresaTipoImpuesto" type="timestamp">
           <column name="fechaHoraInhabilitacionEmpresaTipoImpuesto"/>
        </property>    
        <many-to-one name="tipoImpuesto" class="sistemapagoimpuestos.Entity.TipoImpuesto" column="OIDTipoImpuesto" not-null="true"/>
        <many-to-one name="empresa" class="sistemapagoimpuestos.Entity.Empresa" column="OIDEmpresa" not-null="true"/>
    </class>
</hibernate-mapping>
